\mode<presentation>
% https://www.r-bloggers.com/2011/11/create-your-own-beamer-template/

\RequirePackage{color}

\definecolor{GOE}{RGB}{35,95,150}
\definecolor{LG}{RGB}{211,211,211}
\definecolor{LLG}{RGB}{235,235,235}
\definecolor{LLLG}{RGB}{245,245,245}
\definecolor{DG}{RGB}{130,130,130}

% Settings
\setbeamercolor*{title}{fg=GOE}
\setbeamercolor*{author}{fg=GOE}
\setbeamercolor*{date}{fg=GOE}
\setbeamercolor{frametitle}{fg = GOE}
\setbeamercolor{block title}{fg = GOE,bg=LLG}
% For background: \setbeamercolor{block title}{fg = GOE, bg = LG}
\setbeamercolor{block body}{bg=LLLG}


\setbeamercolor{itemize item}{fg=GOE}%

%\setbeamercolor{block title}{bg=LLG}



\mode<all>


